sg.ShadowQuality=4 ; 阴影质量
sg.PostProcessQuality=2 ; 后处理效果质量
sg.TextureQuality=3 ; 纹理质量
sg.EffectsQuality=2 ; 特效质量
sg.FoliageQuality=2 ; 植被质量
sg.ShadingQuality=4 ; 着色质量
sg.KuroRenderQuality=11 ; "Kuro"项目渲染质量
sg.RayTracingQuality=0 ; 光线追踪质量

[/Script/Engine.GameUserSettings]
bUseVSync=True ; 垂直同步=启用，锁定帧率与刷新率同步（防撕裂）
bUseDynamicResolution=False ; 动态分辨率=禁用，保持固定分辨率输出
ResolutionSizeX=2408 ; 分辨率宽度=2408像素（主屏幕横向像素）
ResolutionSizeY=1080 ; 分辨率高度=1080像素（主屏幕纵向像素）
LastUserConfirmedResolutionSizeX=2408 ; 上次确认的分辨率宽度=2408
LastUserConfirmedResolutionSizeY=1080 ; 上次确认的分辨率高度=1080
WindowPosX=-1 ; 窗口位置X坐标=-1（居中或默认）
WindowPosY=-1 ; 窗口位置Y坐标=-1（居中或默认）
FullscreenMode=1 ; 全屏模式=1（独占全屏），禁用窗口化
GameQualitySettingLevel=5 ; 全局画质等级（自定义预设）
LastConfirmedFullscreenMode=1 ; 上次确认的全屏模式
PreferredFullscreenMode=0 ; 偏好全屏模式
Version=5 ; 配置版本=5（引擎兼容标识）
AudioQualityLevel=0 ; 音频质量，启用3D音效与高码率采样
LastConfirmedAudioQualityLevel=0 ; 上次确认的音频质量=0
FrameRateLimit=60.000000 ; 帧率限制=60帧/秒，锁定最大刷新率
DesiredScreenWidth=1280 ; 期望屏幕宽度=1280
bUseDesiredScreenHeight=False ; 使用期望屏幕高度=禁用，仅宽度生效
DesiredScreenHeight=720 ; 期望屏幕高度=720
LastUserConfirmedDesiredScreenWidth=1280 ; 上次确认的期望宽度=1280
LastUserConfirmedDesiredScreenHeight=720 ; 上次确认的期望高度=720
LastRecommendedScreenWidth=-1.000000 ; 上次推荐的屏幕宽度=-1（未记录）
LastRecommendedScreenHeight=-1.000000 ; 上次推荐的屏幕高度=-1（未记录）
LastCPUBenchmarkResult=-1.000000 ; 上次CPU基准测试结果=-1（未运行测试）
LastGPUBenchmarkResult=-1.000000 ; 上次GPU基准测试结果=-1（未运行测试）
